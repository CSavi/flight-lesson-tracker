# form partial for instructor

<%= form_for(@instructor) do |f| %>
  <%= f.label 'Name: ' %>
  <%= f.text_field :name, class: 'form-field', placeholder: 'Name' %>
  
  <%= f.text_field :cfi, class: 'form-field', placeholder: 'CFI Number' %>

  <%= f.submit 'Create Instructor', class: 'form-submit' %>
<% end %>