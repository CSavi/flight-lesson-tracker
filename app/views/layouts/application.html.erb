<!DOCTYPE html>
<html>
  <head>
    <title>FlightLessonTracker</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <header>
      <nav>
        <ul>
        <li><%= link_to 'Flight Lesson Tracker', root_url %></li>
        <li><%= link_to 'Student List', students_path %></li>
        <li><%= link_to 'Instructor Sign Up', new_instructor_path %></li>
        <li><%= link_to 'Log In', '/login' %></li>
        <li><%= link_to 'Lessons', lessons_path %></li>
        <li><%= link_to 'Log Out', '/logout' %></li>
        </ul>  
    </header>
    <div class="wrapper">
    
      <%= yield %>

    </div>

  </body>
</html>
